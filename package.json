{
  "name": "inquirer-input-table",
  "version": "1.0.0",
  "description": "A table-like input prompt for Inquirer",
  "main": "index.js",
  "scripts": {
    "test": "npm run format",
    "format": "prettier --write \"./**/*.{js,jsx,md,html}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,md,html}": [
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JessieHelloWorld/inquirer-input-table.git"
  },
  "keywords": [
    "inquirer",
    "cli",
    "prompt",
    "input",
    "table"
  ],
  "author": "jessie.zhou<jessiezhoueasc@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JessieHelloWorld/inquirer-input-table/issues"
  },
  "homepage": "https://github.com/JessieHelloWorld/inquirer-input-table#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-cursor": "^3.1.0",
    "cli-table": "^0.3.6",
    "inquirer": "^8.1.2",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  }
}
